%%%%%%% 问卷调查选项格式
   \newlength\chfour\newlength\chtwo\newlength\chthree
    \setlength\chfour{9\ccwd}
    \setlength\chthree{12\ccwd}
    \setlength\chtwo{18\ccwd}

    %\show\chone\show\chtwo\show\chthree
    %\ifthenelse{\chthree<\chtwo}{Yes}{No}

\def\mychA{A}
\def\mychB{B}
\def\mychC{C}
\def\mychD{D}
    \def\myyback{-12pt}
    \newlength\chA\newlength\chB\newlength\chC\newlength\chD \newlength\chtest
    \newlength\chAB\newlength\chCD\newlength\chAD\newlength\chBC
    \newcommand{\choneintwoft}[2]
        {\par\noindent
        \begin{tabular}{*{2}{@{}p{.5\textwidth}}}
        \noindent \mychA.#1&\mychB.#2
        \end{tabular}}
    \newcommand{\choneintwosd}[2]
        {\par\noindent
        \begin{tabular}{@{}p{\textwidth}}
        \noindent \mychA.#1\\[\myyback]
        \noindent \mychB.#2
        \end{tabular}}

    \newcommand{\choneinthreeft}[3]
        {\par\noindent
        \begin{tabular}{*{3}{@{}p{.33\textwidth}}}
           \noindent \mychA.#1&\mychB.#2&\mychC.#3
        \end{tabular}
        }
    \newcommand{\choneinthreesd}[3]{\par
        \noindent\begin{tabular}{*{2}{@{}p{.49\textwidth}}}
            \noindent \mychA.#1&\mychB.#2\\[\myyback]
            \multicolumn{2}{@{}p{\textwidth}}{\noindent \mychC.#3}
        \end{tabular}}
    \newcommand{\choneinthreetd}[3]
        {\par\noindent\begin{tabular}{*{2}{@{}p{.5\textwidth}}}
            \multicolumn{2}{@{}p{\textwidth}}{\noindent \mychA.#1}\\[\myyback]
            \noindent \mychB.#2&\mychC.#3
        \end{tabular}}
    \newcommand{\choneinthreefh}[3]
        {\par\noindent\begin{tabular}{@{}p{\textwidth}}
            \noindent \mychA.#1\\[\myyback] \noindent \mychB.#2\\[\myyback] \noindent \mychC.#3
        \end{tabular}}
    \newcommand{\choneinfourft}[4]
        {\par\noindent
        \begin{tabular}{*{4}{@{}p{.25\textwidth}}}
                \mychA.#1&\mychB.#2&\mychC.#3&\mychD.#4
            \end{tabular}}
    \newcommand{\choneinfoursd}[4]{\par\noindent
        \begin{tabular}{*{2}{@{}p{.5\textwidth}}}
            \mychA.#1&\mychB.#2\\[\myyback]\mychC.#3&\mychD.#4
        \end{tabular}}
    \newcommand{\choneinfourtd}[4]{\par\noindent
        \begin{tabular}{*{2}{@{}p{.5\textwidth}}}
            \mychA.#1&\mychB.#2\\[\myyback]
            \multicolumn{2}{@{}p{\textwidth}}{\mychC.#3}\\[\myyback]
            \multicolumn{2}{@{}p{\textwidth}}{\mychD.#4}
        \end{tabular}}
    \newcommand{\choneinfourfh}[4]{\par\noindent
        \begin{tabular}{*{2}{@{}p{.5\textwidth}}}
            \multicolumn{2}{@{}p{\textwidth}}{\mychA.#1}\\[\myyback]
            \multicolumn{2}{@{}p{\textwidth}}{\mychB.#2}\\[\myyback]
            \mychC.#3&\mychD.#4
        \end{tabular}}
    \newcommand{\choneinfourff}[4]{\par\noindent
        \begin{tabular}{*{2}{@{}p{.5\textwidth}}}
            \multicolumn{2}{@{}p{\textwidth}}{\mychA.#1}\\[\myyback]
            \mychB.#2&\mychC.#3\\[\myyback]
            \multicolumn{2}{@{}p{\textwidth}}{\mychD.#4}
        \end{tabular}}


    \newcommand{\choneinfoursx}[4]{\par\noindent
        \begin{tabular}{@{}p{\textwidth}}
        \mychA.#1\\[\myyback]\mychB.#2\\[\myyback]\mychC.#3\\[\myyback]\mychD.#4
        \end{tabular}}

    \newcommand{\choice}[4][]
        {
        \settowidth{\chD}{\mychD.#1}
        \settowidth{\chA}{\mychA.#2}
        \settowidth{\chB}{\mychB.#3}
        \settowidth{\chC}{\mychC.#4}
        \settowidth{\chtest}{D.}
        \ifthenelse{\chA>\chB}{\chAB=\chA}{\chAB=\chB}
        \ifthenelse{\chC>\chD}{\chCD=\chC}{\chCD=\chD}
        \ifthenelse{\chB>\chC}{\chBC=\chB}{\chBC=\chC}
        \ifthenelse{\chAB>\chCD}{\chAD=\chAB}{\chAD=\chCD}
\ifthenelse{\chC<\chtest}
    {\ifthenelse{\chAB<\chtwo}{\choneintwoft{#2}{#3}}
            {\choneintwosd{#2}{#3}}}
    {\ifthenelse{\chD=\chtest}
        {
        \ifthenelse{\chAD<\chthree}{\choneinthreeft{#2}{#3}{#4}}
            {
            \ifthenelse{\chAB<\chtwo}
                {\choneinthreesd{#2}{#3}{#4}}
                {\ifthenelse{\chBC<\chtwo}
                    {\choneinthreetd{#2}{#3}{#4}}
                    {\choneinthreefh{#2}{#3}{#4}}
                }
            }
        }
        {
        \ifthenelse{\chAD<\chfour}
            {\choneinfourft{#2}{#3}{#4}{#1}}
            {
            \ifthenelse{\chAD<\chtwo}
                {\choneinfoursd{#2}{#3}{#4}{#1}}
                {
                \ifthenelse{\chAB<\chtwo}
                    {\choneinfourtd{#2}{#3}{#4}{#1}}
                    {\ifthenelse{\chBC<\chtwo}
                    {\choneinfourff{#2}{#3}{#4}{#1}}
                    {
                    \ifthenelse{\chCD<\chtwo}
                        {\choneinfourfh{#2}{#3}{#4}{#1}}
                        {\choneinfoursx{#2}{#3}{#4}{#1}}
                        }
                    }
                }
            }
        }
    }
    }

\def\chf#1#2#3#4{\choice[#4]{#1}{#2}{#3}}
%%%% 调用格式 三个选项 \choice{选项1}{选项2}{选项3}
%%%%          四个选项 \chf{选项1}{选项2}{选项3}{选项4}
